type (
	OrderReq {
		Id string `path:"id"`
	}

	OrderReply {
		Id   string `json:"id"`
		Name string `json:"name"`
	}
)
service order {
	@handler getOrder
	get /api/order/get/:id (OrderReq) returns (OrderReply)
}